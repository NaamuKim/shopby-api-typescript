{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "baseUrl": ".",
    "outDir": "dist/"
  },
  "include": ["ts-codegen/**/*"],
  "exclude": ["node_modules"],
}
